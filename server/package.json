{
  "name": "droidppboard-server",
  "private": true,
  "version": "1.0.0",
  "description": "Leaderboard for Elaina PP Project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ."
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [],
  "author": "Rian8337",
  "license": "MIT",
  "dependencies": {
    "@rian8337/osu-base": "^4.0.0-beta.6",
    "@rian8337/osu-difficulty-calculator": "^4.0.0-beta.6",
    "@rian8337/osu-rebalance-difficulty-calculator": "^4.0.0-beta.6",
    "app-structures": "file:../modules/app-structures",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-form-data": "^2.0.18",
    "express-rate-limit": "^7.1.4",
    "mongodb": "^6.3.0",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-form-data": "^2.0.5",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.14.0",
    "prettier": "^3.0.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2022": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "prefer-const": "warn"
    }
  },
  "prettier": {
    "tabWidth": 4
  }
}
